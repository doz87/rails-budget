<div id="budget-container" class="col-xs-12">
  <h1>Incomes</h1>

  <nav>
    <ul>
      <li><%= link_to 'Back', root_path %></li>
      <li><%= link_to 'New', new_income_path %></li>

    </ul>
  </nav>

  <div id="incomes">
    <% @incomes.each do |income| %>

       <div class="income">
         <ul>
           <li><%= link_to(income.name, edit_income_path(income.id)) %></li>
           <li><%= link_to('X', income_path(income.id), :method => :delete) %></li>
         </ul>


       </div>

    <% end %>
  </div>

</div>